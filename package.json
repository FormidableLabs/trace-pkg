{
  "name": "trace-pkg",
  "version": "0.4.1",
  "description": "A dependency tracing packager.",
  "main": "index.js",
  "bin": {
    "trace-pkg": "./bin/trace-pkg.js"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "repository": "https://github.com/FormidableLabs/trace-pkg",
  "author": "Ryan Roemer <ryan.roemer@formidable.com>",
  "license": "MIT",
  "keywords": [
    "dependencies",
    "require",
    "require.resolve",
    "import",
    "serverless",
    "lambda",
    "packaging"
  ],
  "scripts": {
    "preversion": "yarn run check",
    "build:toc": "doctoc --github --notitle --maxlevel 4 README.md",
    "build": "yarn build:toc",
    "lint": "eslint .",
    "test": "mocha \"test/**/*.spec.js\"",
    "test-cov": "nyc mocha \"test/**/*.spec.js\"",
    "check": "yarn lint && yarn test",
    "check-ci": "yarn lint && yarn test-cov",
    "codecov": "codecov"
  },
  "dependencies": {
    "archiver": "^5.3.1",
    "chalk": "^4.1.2",
    "globby": "^11.0.4",
    "jest-worker": "^28.1.1",
    "make-dir": "^3.1.0",
    "trace-deps": "^0.4.10",
    "yaml": "^2.1.1",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/eslint-parser": "^7.18.2",
    "adm-zip": "^0.5.9",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.8.3",
    "doctoc": "^2.2.0",
    "eslint": "^8.18.0",
    "eslint-config-formidable": "^4.0.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-promise": "^6.0.0",
    "execa": "^5.1.1",
    "fs-extra": "^10.1.0",
    "mocha": "^10.0.0",
    "mock-fs": "^5.1.2",
    "nyc": "^15.1.0",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
    "uuid": "^8.3.2"
  }
}
